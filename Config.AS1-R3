no ip domain lookup
ip cef
no ipv6 cef
!
!
multilink bundle-name authenticated
!
interface Loopback0
 ip address 10.1.255.3 255.255.255.255
!
interface FastEthernet0/0
 no ip address
 shutdown
 duplex full
!
interface Ethernet1/0
 description AS1-R1
 ip address 10.1.0.6 255.255.255.252
 ip ospf network point-to-point
 duplex full
 !interface Ethernet1/1
 description AS1-R4
 ip address 10.1.0.9 255.255.255.252
 ip ospf network point-to-point
 duplex full
!
interface Ethernet1/2
 description AS1-R2
 ip address 192.168.12.2 255.255.255.252
 ip ospf network point-to-point
 duplex full

 !router ospf 1
 router-id 3.3.3.3
 passive-interface Loopback0
 network 10.1.0.4 0.0.0.3 area 0
 network 10.1.0.8 0.0.0.3 area 0
 network 10.1.255.3 0.0.0.0 area 0
 network 192.168.12.0 0.0.0.3 area 0
!
router bgp 1
 template peer-policy TEMPLATE-POLICY-AS1
  route-reflector-client
  next-hop-self
  send-community both
 exit-peer-policy
 !
   !
 template peer-session TEMPLATE-SESSION-AS1
  remote-as 1
  update-source Loopback0
 exit-peer-session
 !
 bgp router-id 192.168.168.3
 bgp cluster-id 0.0.0.35
 bgp log-neighbor-changes
 neighbor 10.1.255.1 inherit peer-session TEMPLATE-SESSION-AS1
 neighbor 10.1.255.1 description AS1-R1
 neighbor 10.1.255.1 inherit peer-policy TEMPLATE-POLICY-AS1
 neighbor 10.1.255.2 inherit peer-session TEMPLATE-SESSION-AS1
 neighbor 10.1.255.2 description AS1-R2
 neighbor 10.1.255.2 inherit peer-policy TEMPLATE-POLICY-AS1
 neighbor 10.1.255.4 inherit peer-session TEMPLATE-SESSION-AS1
 neighbor 10.1.255.4 description AS1-R4
 neighbor 10.1.255.4 inherit peer-policy TEMPLATE-POLICY-AS1
 !ip forward-protocol nd
!
!
no ip http server
no ip http secure-server
!
!         
!
!
control-plane
!
!
line con 0
 stopbits 1
line aux 0
 stopbits 1
line vty 0 4
 login
!
!
end

  

  
